{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.borndigital.be.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.borndigital.be.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "Manrope",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Manrope-ExtraLight.ttf",
                    "weight": 100
                  },
                  {
                    "path": "./assets/fonts/Manrope-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/Manrope-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/Manrope-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Manrope-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/Manrope-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Manrope-ExtraBold.ttf",
                    "weight": 800
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Manrope-ExtraLight.ttf",
              "./assets/fonts/Manrope-Light.ttf",
              "./assets/fonts/Manrope-Regular.ttf",
              "./assets/fonts/Manrope-Medium.ttf",
              "./assets/fonts/Manrope-SemiBold.ttf",
              "./assets/fonts/Manrope-Bold.ttf",
              "./assets/fonts/Manrope-ExtraBold.ttf"
            ]
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "998056ef-f3bb-476f-9c00-40812c03a6db"
      }
    },
    "owner": "borndigital.be"
  }
}
